                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.CityName1, new { id = "CityName1", @class = "form-control UpperCase", value = "@Model.CityName1", placeholder = "City Name", data_toggle = "tooltip", data_placement = "bottom", title = "City Name" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-1 control-label ">State :</label>
