@model Corno.Data.Corno.RegistrationViewModel
@{
    ViewBag.Title = "Edit Registration : " + Model.PrnNo;

    var photoImageSource = "data:image/gif;base64, ";
    if (null != Model.Photo)
    {
        photoImageSource = photoImageSource + Convert.ToBase64String(Model.Photo);
    }

    var document1ImageSource = "data:image;base64, ";
    if (null != Model.Document1)
    {
        document1ImageSource = document1ImageSource + Convert.ToBase64String(Model.Document1);
    }

    var document2ImageSource = "data:image;base64, ";
    if (null != Model.Document2)
    {
        document2ImageSource = document2ImageSource + Convert.ToBase64String(Model.Document2);
    }
    var document3ImageSource = "data:image;base64, ";
    if (null != Model.Document3)
    {
        document3ImageSource = document3ImageSource + Convert.ToBase64String(Model.Document3);
    }
    var document4ImageSource = "data:image;base64, ";
    if (null != Model.Document4)
    {
        document4ImageSource = document4ImageSource + Convert.ToBase64String(Model.Document4);
    }
    var document5ImageSource = "data:image;base64, ";
    if (null != Model.Document5)
    {
        document5ImageSource = document5ImageSource + Convert.ToBase64String(Model.Document5);
    }
}

<style>
    .UpperCase {
        text-transform: uppercase;
    }
</style>
<div class="row">
    <div id="breadcrumb" class="col-md-12">
        <ol class="breadcrumb">
            <li><a href="#">Online Exam</a></li>
            <li><a href="#">Student Form</a></li>
        </ol>
    </div>
</div>
@Html.ValidationSummary(false)

<div class="row">
    <div class="col-xs-12 col-sm-12">
        <div class="box ui-draggable ui-droppable">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-edit large"></i>
                    <span>Edit Student NAD Id :  @Model.Id</span>
                </div>
            </div>
            <div class="box-content">
                @using (Html.BeginForm("Edit", "Registration", FormMethod.Post, new { id = "dd", enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    @Html.HiddenFor(model => model.PrnNo)
                    @Html.HiddenFor(model => model.PrnSerialNo)
                    @Html.HiddenFor(model => model.FacultyId)
                    @Html.HiddenFor(model => model.Photo)
                    @Html.HiddenFor(model => model.Signature)
                    @Html.HiddenFor(model => model.Document1)
                    @Html.HiddenFor(model => model.Document2)
                    @Html.HiddenFor(model => model.Document3)
                    @Html.HiddenFor(model => model.Document4)
                    @Html.HiddenFor(model => model.Document5)
                    <div class="form-horizontal">
                        <div class="box-content">
                            <div class="form-group">
                                <div class="col-sm-2">
                                    <button type="button" value="Index" class="btn btn-primary btn-label-left pull-left" onclick="location.href = '@Url.Action("Index", "Registration")'">
                                        <span><i class="fa fa-list-ul"></i></span>
                                        Back to List
                                    </button>
                                </div>
                                <div class="col-sm-offset-8 col-sm-2">
                                    <button type="submit" value="Edit" title="Save" class="btn btn-primary btn-label-left pull-right">
                                        <span><i class="fa fa-save"></i></span>
                                        Save
                                    </button>
                                </div>
                            </div>
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    <strong>Registration Details For PRN - @Model.PrnNo</strong>
                                </div>
                                <div id="collapse1" class="panel-body panel-collapse">
                                    <div role="tabpanel">
                                        <ul class="nav nav-tabs " role="tablist">
                                            <li role="presentation" class="active">
                                                <a href="#RegistrationInfo" aria-controls="RegistrationInfo"
                                                   role="tab" data-toggle="tab">Registration Info</a>
                                            </li>
                                            <li role="presentation">
                                                <a href="#PersonalInfo" aria-controls="PersonalInfo"
                                                   role="tab" data-toggle="tab">Personal Info</a>
                                            </li>
                                            <li role="presentation">
                                                <a href="#ContactInfo" aria-controls="ContactInfo"
                                                   role="tab" data-toggle="tab">Contact Info</a>
                                            </li>
                                            <li role="presentation">
                                                <a href="#AdditionalInfo" aria-controls="AdditionalInfo"
                                                   role="tab" data-toggle="tab">University Info</a>
                                            </li>
                                            <li role="presentation">
                                                <a href="#EducationalInfo" aria-controls="EducationalInfo"
                                                   role="tab" data-toggle="tab">Educational Info</a>
                                            </li>
                                            <li role="presentation">
                                                <a href="#Others" aria-controls="Others"
                                                   role="tab" data-toggle="tab">Others</a>
                                            </li>
                                        </ul>
                                        <div class="tab-content">
                                            <div role="tabpanel" class="tab-pane active" id="RegistrationInfo">
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Admission Year <code>*</code>:</label>
                                                    <div class="col-sm-4">
                                                        @Html.TextBoxFor(m => m.Year, new { id = "Year", minlength = "4", maxlength = "4", @class = "form-control", value = "@Model.Year", placeholder = "Enter Year", data_toggle = "tooltip", data_placement = "bottom", title = "Year" })
                                                    </div>
                                                    <label class="col-sm-2 control-label">Admission Month <code>*</code>:</label>
                                                    <div class="col-sm-4">
                                                        @Html.TextBoxFor(m => m.Month, new { id = "Month", minlength = "2", maxlength = "2", @class = "form-control", value = "@Model.Month", placeholder = "Enter Month", data_toggle = "tooltip", data_placement = "bottom", title = "Month" })
                                                        @Html.ValidationMessageFor(m => m.Month)
                                                    </div>
                                                </div>
                                                @Html.Partial("Partial/_StudentInfoSelection1")
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label">Student Name <code>*</code>:</label>
                                                    <div class="col-sm-4">
                                                        @Html.TextBoxFor(m => m.StudentName, new { id = "StudentName", onkeyup = "InputToUpper(this);", @class = "form-control UpperCase", value = "@Model.StudentName", placeholder = "Enter Student Name", data_toggle = "tooltip", data_placement = "bottom", title = "Student Name" })
                                                        @Html.ValidationMessageFor(m => m.StudentName)
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-2 control-label">Gender <code>*</code>:</label>
                                                    <div class="col-sm-4 control-label">
                                                        @Html.RadioButton("gender", "M", Model.Gender == "M") Male
                                                        <label class="control-label"></label>
                                                        @Html.RadioButton("gender", "F", Model.Gender == "F") Female
                                                        @Html.ValidationMessageFor(m => m.Gender)
                                                    </div>
                                                </div>
                                                <div class="alert alert-warning col-sm-12">
                                                    <strong>Note : </strong>Student should write his/her name as per the Marksheet of previous qualifying examination. No change in the name will be made there after.
                                                </div>
                                                <div class="form-group">
                                                    <button type="button" class="col-sm-offset-11 col-sm-1 btn btn-circle btn-info btnNext" id="nxt3">Next</button>
                                                </div>
                                            </div>
                                            <div role="tabpanel" class="tab-pane" id="PersonalInfo">
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Local Address :</label>
                                                    <div class="col-sm-4">
                                                        @Html.TextAreaFor(model => model.LocalAddress, new { rows = 5, columns = 80, id = "LocalAddress", @class = "form-control", value = "@Model.LocalAddress", placeholder = " Enter Address with Pincode", data_toggle = "tooltip", data_placement = "bottom", title = " Address" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-1 control-label">DOB :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.Dob, "{0:dd-MMM-yyyy}", new { id = "DOB", value = "@Model.DOB", style = "width: 230px;", data_toggle = "tooltip", data_placement = "bottom", title = "e.g:01-Jan-2016" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-1 control-label ">Mobile :</label>
                                                    <div class="col-sm-3  control-label">
                                                        @Html.TextBoxFor(m => m.Mobile1, new { id = "Mobile1", @class = "form-control", value = "@Model.Mobile1", maxlength = "15", placeholder = "Enter Mobile", data_toggle = "tooltip", data_placement = "bottom", title = "Mobile" })
                                                        @Html.ValidationMessageFor(m => m.Mobile1)
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-1 control-label ">Student Email :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.StudentEmail, new { id = "StudentEmail", @class = "form-control", maxlength = "100", value = "@Model.StudentEmail", placeholder = "Enter Student Email", data_toggle = "tooltip", data_placement = "bottom", title = "Student Email" })
                                                        @Html.ValidationMessageFor(m => m.StudentEmail)
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2  control-label ">Birth Place :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.BirthPlace, new { id = "BirthPlace", @class = "form-control", value = "@Model.BirthPlace", placeholder = "Enter Birth Place", data_toggle = "tooltip", data_placement = "bottom", title = "Birth Place" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label">Birth District :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.BirthPlaceDistrictName, new { id = "BirthPlaceDistrictName", @class = "form-control", value = "@Model.BirthPlaceDistrictName", placeholder = "Enter Birth Place District", data_toggle = "tooltip", data_placement = "bottom", title = "Birth District" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label"> Birth State :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.BirthPlaceStateName, new { id = "BirthPlaceStateName", @class = "form-control", value = "@Model.BirthPlaceStateName", placeholder = "Enter Birth Place State", data_toggle = "tooltip", data_placement = "bottom", title = "Birth State" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label"> Birth Country :</label>
                                                    <div class="col-sm-3  control-label">
                                                        @Html.TextBoxFor(m => m.BirthPlaceCountryName, new { id = "BirthPlaceCountryName", @class = "form-control", value = "@Model.BirthPlaceCountryName", placeholder = "Enter Birth Place Country", data_toggle = "tooltip", data_placement = "bottom", title = "Birth Country" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">If Backward :</label>
                                                    <div class="col-sm-3">
                                                        @Html.DropDownListFor(model => model.IfBackward, new SelectList(
                                                               new List<Object>{
                                                               new { value =
                                                                   "Yes", text = "Yes"  },

                                                                new { value = "No", text = "No" }
                                                               },
                                "value", "text"), "Select Backward Religion", new { id = "s2_IfBackward" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label ">Minority :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.Minority, new { id = "Minority", @class = "form-control", value = "@Model.Minority", placeholder = "Enter Minority", data_toggle = "tooltip", data_placement = "bottom", title = "Minority" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Religion :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.Religion, new { id = "Religion", @class = "form-control", value = "@Model.Religion", placeholder = "Enter Religion", data_toggle = "tooltip", data_placement = "bottom", title = "Religion" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label ">Caste :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.Caste, new { id = "Caste", @class = "form-control", value = "@Model.Caste", placeholder = "Enter Caste", data_toggle = "tooltip", data_placement = "bottom", title = "Caste" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Nationality :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.Nationality, new { id = "Nationality", @class = "form-control", value = "@Model.Nationality", placeholder = "Enter Nationality", data_toggle = "tooltip", data_placement = "bottom", title = "Nationality" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label  ">Native Place :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.NativePlace, new { id = "NativePlace", @class = "form-control", value = "@Model.NativePlace", placeholder = "Enter Native Place", data_toggle = "tooltip", data_placement = "bottom", title = "Native Place" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label ">Domocile District :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.DomicileDistrictName, new { id = "DomicileDistrictName", @class = "form-control", value = "@Model.DomicileDistrictName", placeholder = "Enter Domocile District", data_toggle = "tooltip", data_placement = "bottom", title = "Domocile District Name" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label ">Domocile State :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.DomicileStateName, new { id = "DomicileStateName", @class = "form-control", value = "@Model.DomicileStateName", placeholder = "Enter Domocile State", data_toggle = "tooltip", data_placement = "bottom", title = "Domocile State Name" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Area :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.RadioButton("Area", "Rural", true) Rural
                                                        <label class="control-label"></label>
                                                        @Html.RadioButton("Area", "Urban", false) Urban
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-2 control-label ">Blood Group :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.BloodGroup, new { id = "Religion", @class = "form-control", value = "@Model.BloodGroup", placeholder = "Enter Blood Group", data_toggle = "tooltip", data_placement = "bottom", title = "Blood Group" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">NAD Id :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.FormNo, new { id = "FormNo", @class = "form-control", value = "@Model.FormNo", placeholder = "Enter NAD Id", data_toggle = "tooltip", data_placement = "bottom", title = "NAD Id" })
                                                        @Html.ValidationMessageFor(m => m.FormNo)
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2">Photo :</label>
                                                    <div class="col-sm-4 pull-left">
                                                        <input type="file" id="UploadPhoto" name="UploadPhoto" title="Image Id" placeholder="Upload Jpeg Image" value="Upload Image">
                                                        <font color="red">Please  Upload JPEG Image.</font> 
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <button type="button" class="col-sm-1 btn btn-circle btn-info btnPrevious">Previous</button>
                                                    <button type="button" class="col-sm-offset-10 col-sm-1 btn btn-circle btn-info btnNext" id="nxt">Next</button>

                                                </div>
                                            </div>
                                            <div role="tabpanel" class="tab-pane" id="ContactInfo">
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Father Name  :</label>
                                                    <div class="col-sm-4">
                                                        @Html.TextBoxFor(m => m.FatherName, new { id = "FatherName", @class = "form-control UpperCase", value = "@Model.FatherName", placeholder = "Enter Father Name", data_toggle = "tooltip", data_placement = "bottom", title = "Father Name" })
                                                    </div>
                                                    <label class="col-sm-2 control-label col-sm-offset-1">Mother Name  :</label>
                                                    <div class="col-sm-3">
                                                        @Html.TextBoxFor(m => m.MotherName, new { id = "MotherName", @class = "form-control UpperCase", value = "@Model.MotherName", placeholder = "Enter Mother Name", data_toggle = "tooltip", data_placement = "bottom", title = "Mother Name" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Permanent Add. :</label>
                                                    <div class="col-sm-4">
                                                        @Html.TextAreaFor(model => model.PermanentAddress, new { rows = 5, columns = 60, id = "PermanentAddress", @class = "form-control", value = "@Model.PermanentAddress", placeholder = "Enter Address with Pincode", data_toggle = "tooltip", data_placement = "bottom", title = " Address" })
                                                    </div>
                                                    <label class="col-sm-2 control-label col-sm-offset-1 ">City :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.CityName, new { id = "CityName", @class = "form-control UpperCase", value = "@Model.CityName", placeholder = "Enter City Name", data_toggle = "tooltip", data_placement = "bottom", title = "City Name" })
                                                    </div>
                                                    <label class="col-sm-2 control-label col-sm-offset-1">State :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.StateName, new { id = "StateName", @class = "form-control UpperCase", value = "@Model.StateName", placeholder = "Enter State Name", data_toggle = "tooltip", data_placement = "bottom", title = "State Name" })
                                                    </div>

                                                    <label class="col-sm-2 col-sm-offset-1 control-label ">Country :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.CountryName, new { id = "CountryName", @class = "form-control UpperCase", value = "@Model.CountryName", placeholder = "Enter Country Name", data_toggle = "tooltip", data_placement = "bottom", title = "Country Name" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-1 control-label ">Pincode :</label>
                                                    <div class="col-sm-3 control-label">
                                                        @Html.TextBoxFor(m => m.Pincode, new { id = "Pincode", @class = "form-control", value = "@Model.Pincode", maxlength = "6", placeholder = "Enter Pincode", data_toggle = "tooltip", data_placement = "bottom", title = "Pin code" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label ">Email :</label>
                                                    <div class="col-sm-4  control-label">
                                                        @Html.TextBoxFor(m => m.ParentEmail, new { id = "ParentEmail", @class = "form-control", value = "@Model.ParentEmail", placeholder = "Enter Email", data_toggle = "tooltip", data_placement = "bottom", title = "Email" })
                                                    </div>

                                                    <label class="col-sm-2 col-sm-offset-1 control-label  ">Residence Phone :</label>
                                                    <div class="col-sm-3  control-label">
                                                        @Html.TextBoxFor(m => m.ParentTelephone, new { id = "ResidenceTelephone", @class = "form-control", value = "@Model.ParentTelephone", placeholder = "Enter Residence Telephone", data_toggle = "tooltip", data_placement = "bottom", title = "Residence Telephone" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label">Parent Qualification :</label>
                                                    <div class="col-sm-4  control-label">
                                                        @Html.TextBoxFor(m => m.ParentQualification, new { id = "ParentQualification", @class = "form-control", value = "@Model.ParentQualification", placeholder = "Enter Parent Qualification", data_toggle = "tooltip", data_placement = "bottom", title = "Parent Qualification" })
                                                    </div>
                                                    <label class="col-sm-2 col-sm-offset-1 control-label ">Parent Mobile No :</label>
                                                    <div class="col-sm-3  control-label">
                                                        @Html.TextBoxFor(m => m.ParentMobileNo, new { id = "ParentMobileNo", @class = "form-control", value = "@Model.ParentMobileNo", placeholder = "Enter Parent Mobile No", data_toggle = "tooltip", data_placement = "bottom", title = "Parent Mobile No" })
                                                    </div>
                                                </div>
                                                <div class="form-group  has-feedback">
                                                    <label class="col-sm-2 control-label ">Parent Occupation :</label>
                                                    <div class="col-sm-4  control-label">
