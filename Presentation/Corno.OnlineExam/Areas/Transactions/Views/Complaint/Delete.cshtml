@model Corno.Data.Corno.ComplaintViewModel
@{
    ViewBag.Title = "Delete Suggestion Form No : " + Model.Id;
}
<div class="row">
    <div id="breadcrumb" class="col-md-12">
        <ol class="breadcrumb">
            <li><a href="#">Online Exam</a></li>
            <li><a href="#">Delete Suggestion Form</a></li>
        </ol>
    </div>
</div>
@Html.ValidationSummary()
<div class="row">
    <div class="col-xs-12 col-sm-12">
        <div class="box ui-draggable ui-droppable">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-trash-o large"></i>
                    <span>Delete Suggestion Form No :  @Model.Id</span>
                </div>
                <div class="box-icons">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="expand-link">
                        <i class="fa fa-expand"></i>
                    </a>
                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    @Html.HiddenFor(m => m.Id)
                    <div class="form-horizontal">
                        <div class="box-content">
                            <div class="panel panel-info">
                                <div class="panel-heading"><strong>Suggestion Details</strong></div>
                                <div class="panel panel-info">
                                    <div class="panel-heading"><strong>Suggestion Details</strong></div>
                                    <div class="form-group  has-feedback">
                                        <label class="col-sm-2 control-label">Name :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.Name, new { id = "Name", @class = "form-control", value = "@Model.Name", placeholder = "Enter Name", data_toggle = "tooltip", data_placement = "bottom", title = "Name" })
                                        </div>
                                        <label class="col-sm-2 control-label text-right">Assigned To :</label>
                                        <div class="col-sm-4">
                                            @*@Html.DropDownListFor(m => m.AssignedTo, new SelectList(Model.AssignedTo, "ID", "Name"), "Select Assigned To", new { id = "s2_assignedTo", @class = "populate placeholder" })*@
                                            @Html.TextBoxFor(m => m.AssignedTo, new { id = "AssignedTo", @class = "form-control", value = "@Model.AssignedTo", placeholder = "Enter AssignedTo", data_toggle = "tooltip", data_placement = "bottom", title = "AssignedTo" })
                                        </div>
                                    </div>
                                    <div class="form-group  has-feedback">
                                        <label class="col-sm-2 control-label">Assigned By :</label>
                                        <div class="col-sm-4">
                                            @*@Html.DropDownListFor(m => m.AssignedBy, new SelectList(Model.AssignedBy, "ID", "Name"), "Select Assigned By", new { id = "s2_assignedBy", @class = "populate placeholder" })*@
                                            @Html.TextBoxFor(m => m.AssignedBy, new { id = "Name", @class = "form-control", value = "@Model.AssignedBy", placeholder = "Enter AssignedBy", data_toggle = "tooltip", data_placement = "bottom", title = "AssignedBy" })


                                        </div>
                                        <label class="col-sm-2 control-label text-right">Assigned Date :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.AssignedDate, "{0:dd-MMM-yyyy}", new { id = "AssignedDate", @class = "form-control", value = "@Model.AssignedDate", data_toggle = "tooltip", placeholder = "Select Assigned Date", data_placement = "bottom", title = "Assigned Date" })
                                            <span class="fa fa-calendar txt-danger form-control-feedback"></span>
                                        </div>
                                    </div>
                                    <div class="form-group  has-feedback">
                                        <label class="col-sm-2 control-label">Complaint By :</label>
                                        <div class="col-sm-4">
                                            @*@Html.DropDownListFor(m => m.ComplaintBy, new SelectList(Model.ComplaintBy, "ID", "Name"), "Select Complaint By", new { id = "s2_complaintBy", @class = "populate placeholder" })*@
                                            @Html.TextBoxFor(m => m.ComplaintBy, new { id = "ComplaintBy", @class = "form-control", value = "@Model.ComplaintBy", placeholder = "Enter ComplaintBy", data_toggle = "tooltip", data_placement = "bottom", title = "ComplaintBy" })
                                        </div>
                                        <label class="col-sm-2 control-label text-right">Complaint Date :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.ComplaintDate, "{0:dd-MMM-yyyy}", new { id = "ComplaintDate", @class = "form-control", value = "@Model.ComplaintDate", data_toggle = "tooltip", placeholder = "Select Complaint Date", data_placement = "bottom", title = "Complaint Date" })
                                            <span class="fa fa-calendar txt-danger form-control-feedback"></span>
                                        </div>
                                    </div>
                                    <div class="form-group  has-feedback">
                                        <label class="col-sm-2 control-label">Resolved By :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.ResolvedBy, new { id = "ResolvedBy", @class = "form-control", value = "@Model.ResolvedBy", placeholder = "Enter ResolvedBy", data_toggle = "tooltip", data_placement = "bottom", title = "ResolvedBy" })
                                            @*@Html.DropDownListFor(m => m.ResolvedBy, new SelectList(Model.ResolvedBy, "ID", "Name"), "Select Resolved By", new { id = "s2_resolvedBy", @class = "populate placeholder" })*@
                                        </div>
                                        <label class="col-sm-2 control-label text-right">Resolved Date :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.ResolvedDate, "{0:dd-MMM-yyyy}", new { id = "ResolvedDate", @class = "form-control", value = "@Model.ResolvedDate", data_toggle = "tooltip", placeholder = "Select Resolved Date", data_placement = "bottom", title = "Resolved Date" })
                                            <span class="fa fa-calendar txt-danger form-control-feedback"></span>
                                        </div>

                                    </div>
                                    <div class="form-group  has-feedback">
                                        <label class="col-sm-2 control-label">Closed By :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.ClosedBy, new { id = "ClosedBy", @class = "form-control", value = "@Model.ClosedBy", placeholder = "Enter ClosedBy", data_toggle = "tooltip", data_placement = "bottom", title = "ClosedBy" })
                                            @*@Html.DropDownListFor(m => m.ClosedBy, new SelectList(Model.ClosedBy, "ID", "Name"), "Select Closed By", new { id = "s2_closedBy", @class = "populate placeholder" })*@
                                        </div>
                                        <label class="col-sm-2 control-label text-right">Closed Date :</label>
                                        <div class="col-sm-4">
                                            @Html.TextBoxFor(m => m.ClosedDate, "{0:dd-MMM-yyyy}", new { id = "ClosedDate", @class = "form-control", value = "@Model.ClosedDate", data_toggle = "tooltip", placeholder = "Select Closed Date", data_placement = "bottom", title = "Closed Date" })
                                            <span class="fa fa-calendar txt-danger form-control-feedback"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-2">
                                    <button type="button" value="Index" class="btn btn-primary btn-label-left pull-left" onclick="location.href = '@Url.Action("Index", "Complaint")'">
                                        <span><i class="fa fa-list-ul"></i></span>
                                        Back to List
                                    </button>
                                </div>
                                <div class="col-sm-offset-8 col-sm-2">
                                    <button type="submit" value="Delete" onclick="return confirm('Are you sure you wish to delete this Complaint Form?')" title=" Delete" class="btn btn-danger btn-label-left pull-right">
                                        <span><i class="fa fa-trash-o large"></i></span>
                                        Cancel
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">


    function DemoSelect2() {
        $('#s2_complaintBy').select2({ placeholder: "Select Complaint By" });
        $('#s2_assignedTo').select2({ placeholder: "Select Assigned To" });
        $('#s2_assignedBy').select2({ placeholder: "Select Assigned By" });
        $('#s2_resolvedBy').select2({ placeholder: "Select Resolved By" });
        $('#s2_closedBy').select2({ placeholder: "Select Closed By" });

    }
    $(document).ready(function () {
        // Create Wysiwig editor for textare
        //TinyMCEStart('#wysiwig_simple', null);
        //TinyMCEStart('#Description', 'extreme');
        //// Add tooltip to form-controls
        $('.form-control').tooltip();
        // Initialize datepicker
        $("#ComplaintDate").datepicker({
            changeMonth: true, changeYear: true, yearRange: '1900:+0', dateFormat: 'dd-M-yy'
        });
        $("#AssignedDate").datepicker({
            changeMonth: true, changeYear: true, yearRange: '1900:+0', dateFormat: 'dd-M-yy'
        });
        $("#ResolvedDate").datepicker({
            changeMonth: true, changeYear: true, yearRange: '1900:+0', dateFormat: 'dd-M-yy'
        });
        $("#ClosedDate").datepicker({
            changeMonth: true, changeYear: true, yearRange: '1900:+0', dateFormat: 'dd-M-yy'
        });
        LoadSelect2Script(DemoSelect2);

        //// Load example of form validation
        LoadBootstrapValidatorScript(DemoFormValidator);
    });
</script>
